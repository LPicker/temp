// 生成一个包含 n 个整数的数组，每个元素的取值范围 [2, 32]
function fn(n){
  var arr = [], min = 2, max = 32;
  if(!Number.isInteger(n)){
    console.error("fn 需要一个整型的数字作为参数")
    return arr;
  }
  if(n <= 0){
    console.error("fn 需要一个大于0的整型数字作为参数")
    return arr;
  }
  if(n > (max - min)){
    console.error("n 不能大于 " + (max - min))
    return arr;
  }
  while(arr.length < n){
    var num = Math.round(Math.random()*(max - min) + min);
    if(!arr.includes(num)){
      arr.push(num);
    }
  }
  return arr;
}
