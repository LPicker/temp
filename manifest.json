{
   "name": "LpickerLogin",
   "version": "1.0",
   "description": "Crazy LPicker",
   "content_scripts": [{
      "js": ["jquery-2.1.3.min.js", "CloudSOPLogin.js"],
      "css": ["contentLogin.css"],
      "matches": ["https://*/*"]
   }],
   "browser_action": {
       "default_icon": "logo.png",
       "default_title": "setting",
       "default_popup": "popup.html"
   },
   "permissions": ["storage"],
   "manifest_version": 2
}
